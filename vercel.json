{
  "buildCommand": "npm run build:ci",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "env": {
    "SKIP_ENV_VALIDATION": "true"
  },
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "true"
    }
  },
  "functions": {
    "src/app/api/auth/[...nextauth]/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/trpc/[trpc]/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ]
}