    "@tanstack/react-query": "^5.62.7",
    "@tanstack/react-table": "^8.21.3",
    "@trpc/client": "^11.0.0-rc.673",
    "@trpc/next": "^11.0.0-rc.673",
    "@trpc/react-query": "^11.0.0-rc.673",
    "@trpc/server": "^11.0.0-rc.673",
