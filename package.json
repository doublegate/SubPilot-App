{
  "name": "subpilot-app",
  "version": "1.6.0",
  "private": true,
  "type": "module",
  "description": "SubPilot - Your command center for recurring finances",
  "scripts": {
    "build": "next build",
    "build:ci": "next build --no-lint",
    "dev": "next dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run db:studio\" \"mailhog\"",
    "postinstall": "prisma generate",
    "setup": "./scripts/setup-env.sh",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:seed": "dotenv -e .env.local -- tsx prisma/seed.ts",
    "db:seed:cancellation": "dotenv -e .env.local -- tsx prisma/seed-cancellation-providers.ts",
    "db:seed:pricing": "dotenv -e .env.local -- tsx prisma/seed-pricing-plans.ts",
    "db:seed:categories": "dotenv -e .env.local -- tsx scripts/seed-categories.ts",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset --force",
    "init:admin": "dotenv -e .env.local -- tsx scripts/init-admin.ts",
    "audit:accessibility": "tsx scripts/accessibility-audit.ts",
    "test:production": "tsx scripts/test-production-integrations.ts",
    "test:email": "tsx scripts/test-email-integration.ts",
    "validate:production": "tsx scripts/validate-production-env.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
