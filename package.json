    "@types/eslint": "^8.44.7",
    "@types/node": "^20.18.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
